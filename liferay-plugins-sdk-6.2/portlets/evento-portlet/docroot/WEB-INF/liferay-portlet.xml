<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>cadastro-evento</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>
			/css/cadastroevento/main.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/cadastroevento/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>cadastro-evento-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>agenda-evento</portlet-name>
		<icon>/icon.png</icon>
		<trash-handler>br.com.prodevelopment.lapidarios.portlet.agendaevento.trash.AgendaEventoTrashHandler</trash-handler>
		<workflow-handler>br.com.prodevelopment.lapidarios.portlet.agendaevento.workflow.AgendaEventoWorkflowHandler</workflow-handler>
		<header-portlet-css>
			/css/jquery-ui.css
		</header-portlet-css>
		<header-portlet-css>
			/css/agendaevento/main.css
		</header-portlet-css>
		<footer-portlet-javascript>/js/jquery.maskedinput.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/jquery-ui.js</footer-portlet-javascript>
		<footer-portlet-javascript>
			/js/agendaevento/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>agenda-evento-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>lista-evento</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>
			/css/listaevento/main.css
		</header-portlet-css>
		<header-portlet-css>
			/css/listaevento/main-responsive.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/listaevento/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>lista-evento-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>lista-inscrito-evento</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>
			/css/listainscritoevento/main.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/listainscritoevento/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			lista-inscrito-evento-portlet
		</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>