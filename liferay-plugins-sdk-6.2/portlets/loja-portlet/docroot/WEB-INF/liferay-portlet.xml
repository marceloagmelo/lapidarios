<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>cadastro-loja</portlet-name>
		<icon>/icon.png</icon>
		<asset-renderer-factory>br.com.prodevelopment.lapidarios.portlet.cadastroloja.asset.CadastroLojaAssetRendererFactory</asset-renderer-factory>
		<trash-handler>br.com.prodevelopment.lapidarios.portlet.cadastroloja.trash.CadastroLojaTrashHandler</trash-handler>
		<workflow-handler>br.com.prodevelopment.lapidarios.portlet.cadastroloja.workflow.CadastroLojaWorkflowHandler</workflow-handler>
		<header-portlet-css>
			/css/cadastroloja/main.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/cadastroloja/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>cadastro-loja-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>